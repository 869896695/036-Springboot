<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.LizhigushiDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.LizhigushiEntity" id="lizhigushiMap">
        <result property="biaoti" column="biaoti"/>
        <result property="biaoqian" column="biaoqian"/>
        <result property="gushijianjie" column="gushijianjie"/>
        <result property="gushineirong" column="gushineirong"/>
        <result property="fabushijian" column="fabushijian"/>
        <result property="fengmian" column="fengmian"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.LizhigushiVO" >
		SELECT * FROM lizhigushi  lizhigushi         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.LizhigushiVO" >
		SELECT  lizhigushi.* FROM lizhigushi  lizhigushi 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.LizhigushiView" >

		SELECT  lizhigushi.* FROM lizhigushi  lizhigushi 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.LizhigushiView" >
		SELECT * FROM lizhigushi  lizhigushi <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
